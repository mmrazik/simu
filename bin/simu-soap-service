#!/usr/bin/env python

import sys
import os

print os.geteuid()

BASEDIR = os.path.dirname(__file__)

# add local simu module
if os.path.isdir(os.path.join(BASEDIR, '../simu')):
    sys.path.insert(0, os.path.join(BASEDIR, '..'))

from simu import simu_soap_service

if __name__ == "__main__":
    sys.exit(simu_soap_service())
